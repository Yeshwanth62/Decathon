# API Configuration
REACT_APP_API_URL=https://api.sanjeevani-health.com
REACT_APP_SOCKET_URL=https://api.sanjeevani-health.com

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=${REACT_APP_FIREBASE_API_KEY}
REACT_APP_FIREBASE_AUTH_DOMAIN=${REACT_APP_FIREBASE_AUTH_DOMAIN}
REACT_APP_FIREBASE_PROJECT_ID=${REACT_APP_FIREBASE_PROJECT_ID}
REACT_APP_FIREBASE_STORAGE_BUCKET=${REACT_APP_FIREBASE_STORAGE_BUCKET}
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=${REACT_APP_FIREBASE_MESSAGING_SENDER_ID}
REACT_APP_FIREBASE_APP_ID=${REACT_APP_FIREBASE_APP_ID}

# Google Maps Configuration
REACT_APP_GOOGLE_MAPS_API_KEY=${REACT_APP_GOOGLE_MAPS_API_KEY}

# Feature Flags
REACT_APP_ENABLE_VOICE_COMMANDS=true
REACT_APP_ENABLE_3D_MODELS=true
REACT_APP_ENABLE_EMERGENCY=true

# Performance
REACT_APP_MAX_3D_QUALITY=high

# Analytics
REACT_APP_GA_TRACKING_ID=${REACT_APP_GA_TRACKING_ID}

# Error Tracking
REACT_APP_SENTRY_DSN=${REACT_APP_SENTRY_DSN}

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
