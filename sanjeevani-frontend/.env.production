# API Configuration
REACT_APP_API_URL=https://api.sanjeevani-health.com
REACT_APP_SOCKET_URL=https://api.sanjeevani-health.com

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=demo-key
REACT_APP_FIREBASE_AUTH_DOMAIN=demo-domain
REACT_APP_FIREBASE_PROJECT_ID=demo-project
REACT_APP_FIREBASE_STORAGE_BUCKET=demo-bucket
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=demo-sender
REACT_APP_FIREBASE_APP_ID=demo-app-id

# Google Maps Configuration
REACT_APP_GOOGLE_MAPS_API_KEY=demo-maps-key

# Feature Flags
REACT_APP_ENABLE_VOICE_COMMANDS=true
REACT_APP_ENABLE_3D_MODELS=true
REACT_APP_ENABLE_EMERGENCY=true

# Performance
REACT_APP_MAX_3D_QUALITY=high

# Analytics
REACT_APP_GA_TRACKING_ID=demo-ga-id

# Error Tracking
REACT_APP_SENTRY_DSN=demo-sentry-dsn

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
